<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>DisplayCars</servlet-name>
        <servlet-class>Azmin.SERVLETS.DisplayCars</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserInformation</servlet-name>
        <servlet-class>Azmin.SERVLETS.UserInformation</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DisplayBooking</servlet-name>
        <servlet-class>Azmin.SERVLETS.DisplayBooking</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StoreUsers</servlet-name>
        <servlet-class>Azmin.SERVLETS.StoreUsers</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StoreBookings</servlet-name>
        <servlet-class>Azmin.SERVLETS.StoreBookings</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DisplayCars</servlet-name>
        <url-pattern>/DisplayCars</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserInformation</servlet-name>
        <url-pattern>/UserInformation</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DisplayBooking</servlet-name>
        <url-pattern>/DisplayBooking</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StoreUsers</servlet-name>
        <url-pattern>/StoreUsers</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StoreBookings</servlet-name>
        <url-pattern>/StoreBookings</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
